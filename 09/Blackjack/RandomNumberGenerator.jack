class RandomNumberGenerator {
  function int getRand(int seed){
    var int a,b,c,i,rand;
    let i = 0;
    let a = 7;
    let b = 397;
    let rand = seed;
    while (i < 1) {
      // Modulo function
      let rand = rand * a;
      while ( rand > b ) {
        let rand = rand - b;
      }
      let i = i+1;
    }
    while (rand > 52) {
      let rand = rand - 52;
    }
    return rand;
  }
}
